{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "bacon/bacon-qr-code": "^3.0",
        "barryvdh/laravel-debugbar": "^3.13",
        "barryvdh/laravel-dompdf": "^3.1",
        "kkomelin/laravel-translatable-string-exporter": "^1.22",
        "konekt/html": "^6.5",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/cashier-paddle": "^1.9",
        "laravel/framework": "^11.9",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.28",
        "maatwebsite/excel": "^3.1",
        "milon/barcode": "^11.0",
        "munafio/chatify": "^1.5",
        "php-open-source-saver/jwt-auth": "^2.6",
        "rachidlaasri/laravel-installer": "^4.1",
        "santigarcor/laratrust": "^8.3",
        "workdo/account": "dev-main",
        "workdo/hrm": "dev-main",
        "workdo/landing-page": "dev-main",
        "workdo/lead": "dev-main",
        "workdo/paypal": "dev-main",
        "workdo/pos": "dev-main",
        "workdo/product-service": "dev-main",
        "workdo/stripe": "dev-main",
        "workdo/taskly": "dev-main",
        "yajra/laravel-datatables": "^11.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Workdo\\": "packages/workdo/",
            "Workdo\\LandingPage\\": "packages/workdo/LandingPage/src/",
            "Workdo\\ProductService\\": "packages/workdo/ProductService/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/Helper/helper.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [{
        "type": "path",
        "url": "packages/workdo/*",
        "options": {
            "symlink": true
        }
    }]
}
